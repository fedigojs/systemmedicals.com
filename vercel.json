{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://systemmedicals.com/$1",
      "permanent": false,
      "has": [
        { "type": "host", "value": ".*vercel\\.app" }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://systemmedicals.com/$1",
      "permanent": true,
      "has": [
        { "type": "host", "value": "www.systemmedicals.com" }
      ]
    }
  ]
}